{
  "consumer": {
    "name": "Foo"
  },
  "interactions": [
    {
      "description": "a retrieve thing request",
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/thing"
      },
      "response": {
        "body": {
          "company": "My big company",
          "factories": [
            {
              "capacity": 5,
              "location": "Sydney"
            }
          ]
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.company": {
            "match": "type"
          },
          "$.body.factories": {
            "match": "type",
            "min": 1
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.4.22",
      "mockserver": "1.2.9",
      "models": "1.2.3"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "Bar"
  }
}